{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": "current"
      }
    }],
    "@babel/typescript"
  ],
  "plugins": [
    ["module-resolver", {
      "extensions": [".js", ".ts"],
      "root": [
        "./src"
      ],
      "alias": {
        "~": "./src/",
        "@ouka": "./src/lib/"
      }
    }],
    "@babel/plugin-transform-typescript",
    "@babel/plugin-proposal-class-properties"
  ]
}
